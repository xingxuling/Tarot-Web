{
  "expo": {
    "name": "Tarot Assistant",
    "slug": "tarot-assistant",
    "android": {
      "config": {
        "googleMobileAdsAppId": "@env:ADMOB_APP_ID"
      },
      "package": "com.tarotassistant.app",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "versionCode": 1
    },
    "ios": {
      "config": {
        "googleMobileAdsAppId": "@env:ADMOB_APP_ID"
      },
      "bundleIdentifier": "com.tarotassistant.app",
      "buildNumber": "1.0.0"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tarotassistant",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#1e1b4b",
    "primaryColor": "#9333ea",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e1b4b"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
